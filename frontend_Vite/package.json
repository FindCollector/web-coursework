{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:coverage": "VITE_COVERAGE=true vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:coverage": "CYPRESS_COVERAGE=true cypress run --browser chrome --reporter mochawesome",
    "merge:reports": "mochawesome-merge cypress/reports/mochawesome-report/*.json > cypress/reports/output.json",
    "merge:coverage": "nyc merge coverage .nyc_output/out.json && nyc report --reporter=html --reporter=text --reporter=json-summary",
    "report": "npm run test:coverage && (npm run dev:coverage & sleep 5 && npm run cypress:coverage && kill $(lsof -t -i:5173) && npm run merge:coverage)"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fullcalendar/core": "^6.1.17",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/react": "^6.1.17",
    "@fullcalendar/timegrid": "^6.1.17",
    "@hookform/resolvers": "^4.1.3",
    "@react-oauth/google": "^0.12.1",
    "@reduxjs/toolkit": "^2.6.1",
    "@tanstack/react-query": "^5.69.0",
    "antd": "^5.24.5",
    "axios": "^1.8.4",
    "canvas-confetti": "^1.9.3",
    "framer-motion": "^12.6.2",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.54.2",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.4.0",
    "three": "^0.176.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.0",
    "@eslint/js": "^9.21.0",
    "@tailwindcss/postcss": "^4.0.17",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-istanbul": "^1.6.1",
    "autoprefixer": "^10.4.21",
    "cypress": "^13.6.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "istanbul-lib-coverage": "^3.2.2",
    "jsdom": "^24.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0",
    "msw": "^2.2.2",
    "nyc": "^17.1.0",
    "postcss": "^8.5.3",
    "react-dnd-test-backend": "^16.0.1",
    "tailwindcss": "^4.0.17",
    "vite": "^6.2.0",
    "vite-plugin-istanbul": "^7.0.0",
    "vitest": "^1.5.0"
  }
}
