{
  "all": true,
  "include": [
    "src/**/*.{js,jsx,ts,tsx}"
  ],
  "exclude": [
    "**/*.d.ts",
    "**/*.test.{js,jsx,ts,tsx}",
    "**/*.spec.{js,jsx,ts,tsx}",
    "tests/**/*.*",
    "node_modules/**/*.*",
    "cypress/**/*.*"
  ],
  "reporter": [
    "html",
    "text",
    "text-summary",
    "json-summary"
  ],
  "check-coverage": true,
  "statements": 0,
  "branches": 0,
  "functions": 0,
  "lines": 0
} 